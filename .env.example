# NASA Meteor Mastery Environment Configuration
# Copy this file to .env and update the values

# ========== API KEYS ==========
NASA_API_KEY=your_nasa_api_key_here
USGS_API_KEY=your_usgs_api_key_optional

# ========== DATABASE ==========
MONGODB_URI=mongodb://localhost:27017/meteor-mastery
REDIS_URL=redis://localhost:6379

# ========== SERVER CONFIG ==========
NODE_ENV=development
PORT=5000
FRONTEND_PORT=3000

# ========== SECURITY ==========
JWT_SECRET=your_jwt_secret_key_here
SESSION_SECRET=your_session_secret_here
CORS_ORIGIN=http://localhost:3000

# ========== CACHE CONFIG ==========
CACHE_TTL=300000
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=100

# ========== LOGGING ==========
LOG_LEVEL=info
LOG_FILE=logs/app.log

# ========== EXTERNAL SERVICES ==========
SENTRY_DSN=your_sentry_dsn_optional
NEW_RELIC_LICENSE_KEY=your_new_relic_key_optional

# ========== SIMULATION DEFAULTS ==========
DEFAULT_METEOR_SIZE=100
DEFAULT_METEOR_DENSITY=3000
DEFAULT_IMPACT_VELOCITY=17000
DEFAULT_IMPACT_ANGLE=45