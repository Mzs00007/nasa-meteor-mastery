{
  "name": "meteor-madness",
  "version": "1.0.0",
  "private": true,
  "description": "Meteor Madness - NASA/USGS Asteroid Impact Simulator",
  "main": "src/App.js",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "websocket": "node websocket-server.js",
    "dev": "concurrently \"npm run websocket\" \"npm start\"",
    "lint": "eslint src/ __tests__/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ __tests__/ --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src/ __tests__/ --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"__tests__/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\" \"__tests__/**/*.{js,jsx,ts,tsx}\"",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "npm test -- --coverage --watchAll=false --ci",
    "test:unit": "npm test -- --testPathPattern=__tests__/components",
    "test:integration": "npm test -- --testPathPattern=__tests__/integration",
    "test:backend": "npm test -- --testPathPattern=__tests__/backend",
    "quality:check": "npm run lint:check && npm run format:check && npm run test:ci",
    "quality:fix": "npm run lint:fix && npm run format",
    "pre-commit": "npm run quality:check",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "cesium": "^1.134.0",
    "cors": "^2.8.5",
    "css-select": "^5.2.2",
    "d3": "^7.8.5",
    "express": "^5.1.0",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "jszip": "^3.10.1",
    "nth-check": "^2.1.1",
    "openmct": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.3",
    "react-scripts": "^5.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.19",
    "three": "^0.155.0",
    "topojson-client": "^3.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@eslint/js": "^9.36.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-testing-library": "^7.11.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17"
  }
}
